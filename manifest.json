{
    "manifest_version": 3,
    "name": "Remove Watermark",
    "version": "1.0",
    "description": "ลบ div class='ip-watermark' ออกจากหน้าเว็บ",
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  